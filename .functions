#Create a new directory and enter it
function mk() {
  mkdir -p "$@" && cd "$@"
}
# Open man page as PDF
function manpdf() {
  man -t "${1}" | open -f -a /Applications/Preview.app/
}
gitnewBranch() {
	git checkout main
	git pull origin main
	git checkout -b $1 main
}

aliUp() {
	aliyunpan upload $1 /temp
}

ln2path() {
	local target_file=$1
	# Check if the target file exists
  	if [ ! -f "$target_file" ]; then
    		echo "Error: Target file does not exist."
    	return 1
  	fi
	ln -s $(pwd)/$target_file /home/ws/.local/bin
}

ln2desk() {
	local target_file=$1
	# Check if the target file exists
  	if [ ! -f "$target_file" ]; then
    		echo "Error: Target file does not exist."
    	return 1
  	fi
	ln -s $(pwd)/$target_file /home/ws/Desktop
}
proj3_diff() {
	python3 binary_to_hex_cpu.py $1 > student.out
	python3 binary_to_hex_cpu.py $2 > reference.out
	diff student.out reference.out
}
